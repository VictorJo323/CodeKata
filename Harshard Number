public class Solution 
{
    public bool solution(int x) 
    {
        int temp = x;
        int y = 0;
        
        while (temp > 0)
        {
            y += temp%10;
            temp = temp/10;
        }
        if (x%y == 0)
        {
            return true;
        }
        else
        {
            return false;
        }
    }
}
